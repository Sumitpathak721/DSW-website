<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="/static/style/index.css">
    <link rel="stylesheet" href="/static/style/style.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.4/jquery.min.js"></script>
    <title>DSW</title>
    <style>
        body{
           background-image:url(`luke-chesser-3rWagdKBF7U-unsplash.jpg`);
        }
    </style>
</head>
<script>
    window.SERVER_DIR = '<%=SERVER_DIR%>';
</script>
<!-- Popup Background Screen -->
<div id="popup-screen">
</div>
<!-- Main Screen -->
<body onload="onLoad()">
    <nav id="navbar1">
        <div>
            <img src="<%=SERVER_DIR%><%=main.icon%>" alt="logo">
        </div>
        <div id="nav1-links">
            <div id="search">
                <input id="search-input" type="search" name="clubName" onkeyup="searchClubs(this.value)"
                    autocomplete="off">
                <div id="suggestionClubs"></div>
            </div>
            <button id="search-btn-icon">
                <i class="fa fa-search"></i>
            </button>
            <div id="pvtComponent1"></div>
        </div>
    </nav>
    <!-- Notification popup Screen-->
    <div class="detail-container" id="messages-container">
        <div id="message-header">
            <button onclick="removeMessages()"><i class="fa fa-remove"></i></button>
            <h1>Messages</h1>
        </div>
        <div id="messages-list">
        </div>
    </div>
    <!--Video Conatiner  -->
    <div id="main-video">
        <video style="width: 60%; " loop muted autoplay="autoplay">
            <source src="<%=SERVER_DIR%><%=main.video%>" type="video/mp4">
        </video>
    </div>
    <!-- Event popup Screen --->
    <div id="live-events-container">
        <h1 id="head-live-events-container"><i class="fa fa-calendar"></i>Upcoming Events:</h1>
        <div id="events-list">
            <%for(let i=0;i<events.length;i++){%>
                <%let dateString=events[i].Date.split("-");%>
                    <button class="live-event" onclick="eventpop(<%=JSON.stringify(events[i])%>)">
                        <img src="<%=SERVER_DIR%><%=events[i].icon%>" alt="clubIcon">
                        <h4 class="event-description">
                            <%=events[i].Name%>&nbsp;Organised by&nbsp;<%=events[i].clubName%>&nbsp;on&nbsp;
                                    <%=dateString[2]%>-<%=dateString[1]%>-<%=dateString[0]%>
                        </h4>
                    </button>
                    <%}%>
        </div>
    </div>
    <!-- Event Pop screen -->
    <div class="detail-container" id="event-popup">
        <div id="event-popup-container1">
            <button class="remove-icon" onclick="removeEventPopup()"><i class="fa fa-remove"></i></button>
            <img id="event-popup-img" alt="eventIcon">
        </div>
        <div id="event-popup-container2">
            <h1 id="event-popup-name"></h1>
            <h2>Organized By: <span id="event-popup-clubName"></span></h2>
            <h2>Event Date: <span id="event-popup-date"></span></h2>
            <p id="event-popup-detail"></p>
            <a href="/club/ticket" target="_blank" id="event-btn">Get Ticket</a>
        </div>
    </div>
    
<!--     get to know our team         -->


<div class="team">
    <div class="team_heading">“ Get to Know Our Team ”</div>
    <hr>
    <div class="parent">
        <div class="child"><img src="static/images/mkk sir.jpg">
            <h2>Mr. Manish Kaushik</h2>
            <h5>Dean Students' Welfare</h5>
        </div>
        <div class="child"><img src="static/images/kanika mam.jpg">
            
            <h2>Mrs. Kanika Jindal</h2>
            
            <h5>Associate Dean Students' Welfair</h5>
        </div>
    </div>

</div>
</div>



    <!--     End      -->
    <!-- Club of the year detail -->

    <!--coty(Club of the year)-->
    <div class="detail-container" id="coty">
        <div id="coty-container1">
            <h1>Best Club of the Year</h1>
            <h1 id="coty-name">
                <%=coty.name%>
            </h1>
        </div>
        <div id="coty-container2">
            <img id="coty-img" src="<%=SERVER_DIR%><%=coty.icon%>" alt="Club-of-the-year-NIET">
        </div>
    </div>

    <!----------------   Footer    ---------------------------->

    <footer class="new_footer_area bg_color" id="contact">
        <div style="padding-top:20px">
            <div class="ourDevs_heading">
                <h1>Our Developer</h1>
                <hr>
            </div>
            <div id="ourDevs_container">
                <div class="ourDevs_container-2">
                    <div class="ourDevs_details">
                        <img src="/static/images/SumitPathak.jpeg" alt="Sumit Pathak">
                        <div>
                            <a target="_blank" href="https://sumitpathak721.github.io/__sumitpathak__/"><h1>Sumit Pathak</h1></a>
                            <h5>Backend Developer</h5>
                        </div>
                    </div>
                    <div class="ourDevs_details">
                        <img src="/static/images/SushantVerma.jpg" alt="Sushant Verma">
                        <div>
                            <a target="_blank" href="https://www.linkedin.com/in/sushant-verma-b53171226/"><h1>Sushant Verma</h1></a>
                            <h5>FrontEnd Developer</h5>
                        </div>
                    </div>
                </div>
                <div class="ourDevs_container-2">
                    <div class="ourDevs_details">
                        <img src="/static/images/AmaanKhan.jpg" alt="Amaan Khan">
                        <div>
                            <a target="_blank" href="https://www.linkedin.com/in/amaan-khan-ak20121/"><h1>Md Amaan Khan</h1></a>
                            <h5>FrontEnd Developer</h5>
                        </div>
                    </div>
                    <div class="ourDevs_details">
                        <img src="/static/images/YashiGupta.jpeg" alt="Yashi Gupta">
                        <div>
                            <a target="_blank" href="https://www.linkedin.com/in/yashigupta406/"><h1>Yashi Gupta</h1></a>
                            <h5>FrontEnd Developer</h5>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="new_footer_top">
            <div class="container">
                <div class="row">

                    <div class="col-lg-3 col-md-6">
                        <div class="f_widget about-widget pl_70 wow fadeInLeft" data-wow-delay="0.4s"
                            style="visibility: visible; animation-delay: 0.4s; animation-name: fadeInLeft;">
                            <h3 class="f-title f_600 t_color f_size_18">Download</h3>
                            <ul class="list-unstyled f_list">
                                <li><a href="#">Company</a></li>
                                <li><a href="#">Android App</a></li>
                                <li><a href="#">ios App</a></li>
                                <li><a href="#">Desktop</a></li>
                                <li><a href="#">Projects</a></li>
                                <li><a href="#">My tasks</a></li>
                            </ul>
                        </div>
                    </div>

                    <div class="col-lg-3 col-md-6">
                        <div class="f_widget social-widget pl_70 wow fadeInLeft" data-wow-delay="0.8s"
                            style="visibility: visible; animation-delay: 0.8s; animation-name: fadeInLeft;">
                            <h3 class="f-title f_600 t_color f_size_18">Team Solutions</h3>
                            <div class="f_social_icon">
                                <a href="#" class="fa fa-facebook"></a>
                                <a href="#" class="fa fa-twitter"></a>
                                <a href="#" class="fa fa-linkedin"></a>
                                <a href="#" class="fa fa-pinterest"></a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="footer_bg">
                <div class="footer_bg_one"></div>
                <div class="footer_bg_two"></div>
            </div>
        </div>
        
    </footer>
</body>
<script src="/static/js/api/index.js"></script>

</html>